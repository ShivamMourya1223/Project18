<h1>New Form</h1>
<form class="form-inline"
<%=form_for @user  do |f| %>

<div class="form-group">
<%=f.label 'Name'%>
<%=f.text_field :name,class:"form-control"%>
<br><br>
</div>


<div class="form-group">
<%=f.label 'Age'%>
<%=f.text_field :age,class:"form-control"%>
<br><br>
</div>

<div class="form-group">
<%=f.label 'Address'%>
<%=f.text_field :adress,class:"form-control"%>
<br><br>
</div>

<div class="form-group">
<%=f.label 'Email'%>
<%=f.text_field :email,class:"form-control"%>
<br><br>
</div>

<%= f.submit %>

<% end %>
</form>